float Factorial( x : float) {
    if (x >= 1.0){
            return x * Factorial(x - 1.0);
    }else{
            return 1.0;
    }
}

print Factorial(5.0);

float Factorial(x : float, toMakeAnExample : float) {
    if (x >= 1.0){
            return x * Factorial(x - 1.0);
    }else{
            return 1.0;
    }
}


print Factorial(5.0, 1.0);
